<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sprint BY Data Prep Toolbar Suite" preserve-order="true" parallel="tests" thread-count="2">
    <groups>
        <run>
            <include name="DataPrep.*"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="org.alfresco.webdrone.testng.listener.ScreenshotHTMLReporter"/>
    </listeners>

    <test name="Trashcan tests Suite">
        <classes>
            <class name="org.alfresco.share.user.trashcan.TrashcanTest2"/>
        </classes>
    </test>

    <test name="Toolbar tests Suite">
        <classes>
            <class name="org.alfresco.share.site.document.ToolbarTest"/>
        </classes>
    </test>

    <test name="SiteNotice tests Suite">
        <classes>
            <class name="org.alfresco.share.dashlet.SiteNoticetDashletTest">
                <methods>
                    <include name="dataPrep_AONE_13976"/>
                    <include name="dataPrep_AONE_13983"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>