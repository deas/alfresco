<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="WAT2 Enterprise Data Prep Suite" verbose="1" preserve-order="true">
    <groups>
        <run>
            <include name="DataPrep.*" />
            <exclude name="CloudOnly" />
            <exclude name="Hybrid.*" />
        </run>
    </groups>
    <test name="LiveSearchTest Enterprise Data Prep" verbose="1" parallel="classes" thread-count="1">
        <classes>
            <class name="org.alfresco.share.search.LiveSearchTest"></class>
        </classes>
    </test>
</suite>