        <property name="excludedTypes">
            <list>
                <value>{http://www.jcp.org/jcr/1.0}*</value>
                <value>{http://www.jcp.org/jcr/nt/1.0}*</value> 
                <value>{http://www.jcp.org/jcr/mix/1.0}*</value>
                <value>{http://www.jcp.org/jcr/sv/1.0}*</value>
            </list>
        </property>
    </bean>
</beans>