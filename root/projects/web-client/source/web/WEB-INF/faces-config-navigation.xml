   </navigation-rule>
   
   <!-- rule to get back to the login page from anywhere -->
   <navigation-rule>
      <from-view-id>/jsp/*</from-view-id>
      <navigation-case>
         <from-outcome>logout</from-outcome>
         <to-view-id>/jsp/login.jsp</to-view-id>
