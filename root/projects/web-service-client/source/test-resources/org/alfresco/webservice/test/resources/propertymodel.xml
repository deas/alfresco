<model name="wstm:testmodel" xmlns="http://www.alfresco.org/model/dictionary/1.0">

   <description>Alfresco Content Domain Model</description>
   <author>Alfresco</author>
   <published>2002-06-02</published>
   <version>1.0</version>

   <imports>
      <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
      <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
   </imports>
   
   <namespaces>
      <namespace uri="http://www.alfresco.org/model/webservicetestmodel/1.0" prefix="wstm"/>
   </namespaces>
   
   <types>
   
      <type name="wstm:testproperties">
         <title>Test Setting Properties</title>
         <parent>sys:base</parent>
         <properties>
            
            <property name="wstm:textProp">
               <title>TextProp</title>
               <type>d:text</type>
            </property>            
            <property name="wstm:intProp">
               <title>IntProp</title>
               <type>d:int</type>
            </property>            
            <property name="wstm:longProp">
               <title>LongProp</title>
               <type>d:long</type>
            </property>            
            <property name="wstm:floatProp">
               <title>FloatProp</title>
               <type>d:float</type>
            </property>            
            <property name="wstm:doubleProp">
               <title>DoubleProp</title>
               <type>d:double</type>
            </property>            
            <property name="wstm:dateProp">
               <title>DateProp</title>
               <type>d:date</type>
            </property>            
            <property name="wstm:datetimeProp">
               <title>DateTimeProp</title>
               <type>d:datetime</type>
			   <index enabled="true">
				  <atomic>true</atomic>
				  <stored>false</stored> 
                  <tokenised>both</tokenised>
               </index>
            </property>            
            <property name="wstm:booleanProp">
               <title>BooleanProp</title>
               <type>d:boolean</type>
            </property>         
            <property name="wstm:qnameProp">
               <title>QNameProp</title>
               <type>d:qname</type>
            </property>            
            <property name="wstm:noderefProp">
               <title>NodeRefProp</title>
               <type>d:noderef</type>
            </property>
            
            <property name="wstm:textMultiProp">
               <title>TextProp</title>
               <type>d:text</type>
               <multiple>true</multiple>
            </property> 
            
         </properties>
      </type>
      
   </types>
   
</model>   