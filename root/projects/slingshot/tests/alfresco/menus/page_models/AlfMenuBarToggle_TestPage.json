{
   "widgets": [
      {
         "name": "alfresco/menus/AlfMenuBar",
         "config": {
            "widgets": [
               {
                  "name": "alfresco/menus/AlfMenuBarToggle",
                  "config": {
                     "id": "BASIC_MENU_BAR_TOGGLE"
                  }
               },
               {
                  "name": "alfresco/menus/AlfMenuBarToggle",
                  "config": {
                     "id": "MENU_BAR_TOGGLE_CUSTOM_LABEL",
                     "checked": true,
                     "onConfig": {
                        "label": "On (Custom Label)",
                        "publishTopic": "CLICK",
                        "publishPayload": {
                           "clicked": "TOGGLE_WITH_LABEL",
