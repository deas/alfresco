<extension>
   <modules>
      <module>
         <id>Debug Enabler Extension</id>
         <description>Applies config to enable debug</description>
         <configurations>
            <config replace="true">
               <flags>
                  <client-debug>true</client-debug>
                  <client-debug-autologging>false</client-debug-autologging>
               </flags>
            </config>
         </configurations>
      </module>
   </modules>
</extension>