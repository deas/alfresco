<?xml version='1.0' encoding='UTF-8'?>
<theme>
   <title>Yellow Theme</title>
   <title-id>theme.yellowTheme</title-id>
   <css-tokens>

      <less-variables>
      @docListHeaderBgColour: #f2f2f2;
      @docListOddRowColour: #fff;
      @docListEvenRowColour: #f7f7f7;
      @docListBorderColour: #e3e3e3;
      @docListDndHighlight: #4F94C9;

      @selectControlStaticBgColour: #e3e3e3;
      </less-variables>

      <theme-font-family-1>arial,helvetica,clean,sans-serif</theme-font-family-1>
      <theme-font-family-2>arial,helvetica,clean,sans-serif</theme-font-family-2>
      <theme-font-family-3>arial,helvetica,clean,sans-serif</theme-font-family-3>
      <theme-font-size-1>13px</theme-font-size-1>
      <theme-color-1>#f6931c</theme-color-1>
      <theme-color-2>#666666</theme-color-2>
      <theme-color-3>#fff</theme-color-3>
      <theme-color-4>#009300</theme-color-4>
      <theme-color-5>#fff</theme-color-5>
      <theme-color-6>#999999</theme-color-6> <!-- Used for faded fonts -->
      <theme-color-7>#333</theme-color-7> <!-- Used for light on dark fonts -->
      <theme-color-8>#4F94C9</theme-color-8> <!-- Used for highlight colours -->
      <theme-bg-color-1>#FFEE9E</theme-bg-color-1>
      <theme-bg-color-2>#fffdf4</theme-bg-color-2>
      <theme-bg-color-3>#f8f8f8</theme-bg-color-3>
      <theme-bg-color-4>#fff</theme-bg-color-4>
      <theme-bg-color-5>#2FB42C</theme-bg-color-5>
      <theme-bg-color-6>#F4FAFF</theme-bg-color-6>
      <theme-bg-color-7>#F5FFF2</theme-bg-color-7>
      <theme-bg-color-8>#FFFFFF</theme-bg-color-8>
      <theme-bg-color-9>#DCEAF4</theme-bg-color-9> <!--  Used for highlight background (used with theme-color-8) -->
      <theme-bg-1></theme-bg-1>
      <theme-bg-2>background-color: #F8F8F8;background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, #fff),color-stop(1, #f6f6f6));background-image: -moz-linear-gradient(center top,#fff 0%,#f6f6f6 100%);</theme-bg-2>
      <theme-bg-3>background-color: #CCCCCC;background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, rgb(204,204,204)),color-stop(1, rgb(232,232,232)));background-image: -moz-linear-gradient(center top,rgb(204,204,204) 0%,rgb(232,232,232) 100%);</theme-bg-3>
      <theme-bg-4>background-color: #33FF66;background-image: -webkit-gradient(linear,left top,left bottom,color-stop(0, rgb(51,255,102)),color-stop(1, rgb(51,153,102)));background-image: -moz-linear-gradient(top,rgb(51,255,102) 0%,rgb(51,153,102) 100%);</theme-bg-4>
      <theme-bg-selected>background-color: #fff7c9;</theme-bg-selected>
      <theme-border-1>1px solid #468058</theme-border-1>
      <theme-border-2>1px solid #2FB42C</theme-border-2>
      <theme-border-3>1px solid #c6c6c6</theme-border-3>
      <theme-border-4>1px solid #DDD6A0</theme-border-4>
      <theme-border-5>1px solid #c2c2c2</theme-border-5>

      <!-- New theme tokens -->
      <button>background-image: url(themes/yellowTheme/images/sprite.png);border-color: #808080;border-style: solid;border-width: 0;margin: auto 0.25em;color:#333;</button>

   </css-tokens>
</theme>